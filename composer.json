{
    "name": "proklung/twig-extensions-bundle",
    "description": "Custom twig extensions and functions for Twig.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gavrilov Fedy",
            "email": "fedor.gavrilov.75@mail.ru",
            "homepage": "https://github.com/ProklUng"
        }
    ],
    "support": {
        "issues": "https://github.com/ProklUng/twig.extensions.bundle/issues",
        "source": "https://github.com/ProklUng/twig.extensions.bundle"
    },
    "autoload": {
        "psr-4": {
            "Prokl\\TwigExtensionsPackBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "require": {
        "php": ">=7.3",
        "psr/container": "^1.0",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0",
        "symfony/config": "~4|~5",
        "symfony/framework-bundle": "^4.0 || ^5.0",
        "twig/twig": "^1.41|^2.10",
        "twig/cssinliner-extra": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.2 || ^8.0 || ^9.0",
        "proklung/phpunit-testing-tools": "^1.0",
        "symfony/var-dumper": "^4.0 || ^5.0"
    }
}
